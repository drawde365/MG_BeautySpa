<configuration>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="ProductosPortBinding" />
				<binding name="ProductosTipoPortBinding" />
				<binding name="PedidoPortBinding" />
				<binding name="EmpleadoPortBinding" />
				<binding name="CitaPortBinding" />
				<binding name="CalendarioPortBinding" />
				<binding name="UsuarioPortBinding" />
				<binding name="ClientePortBinding" />
				<binding name="ComentarioPortBinding" />
				<binding name="ServicioPortBinding" />
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Productos"
                binding="basicHttpBinding" bindingConfiguration="ProductosPortBinding"
                contract="SoftInvWSProductos.Productos" name="ProductosPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/ProductoTipo"
                binding="basicHttpBinding" bindingConfiguration="ProductosTipoPortBinding"
                contract="SoftInvWSProductoTipo.ProductosTipo" name="ProductosTipoPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Pedido"
                binding="basicHttpBinding" bindingConfiguration="PedidoPortBinding"
                contract="SoftInvWSPedido.Pedido" name="PedidoPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Empleado"
                binding="basicHttpBinding" bindingConfiguration="EmpleadoPortBinding"
                contract="SoftInvWSEmpleado.Empleado" name="EmpleadoPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Cita" binding="basicHttpBinding"
                bindingConfiguration="CitaPortBinding" contract="SoftInvWSCita.Cita"
                name="CitaPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Calendario"
                binding="basicHttpBinding" bindingConfiguration="CalendarioPortBinding"
                contract="SoftInvWSCalendario.Calendario" name="CalendarioPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Usuario"
                binding="basicHttpBinding" bindingConfiguration="UsuarioPortBinding"
                contract="SoftInvWSUsuario.Usuario" name="UsuarioPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Cliente"
                binding="basicHttpBinding" bindingConfiguration="ClientePortBinding"
                contract="SoftInvWSCliente.Cliente" name="ClientePort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Comentario"
                binding="basicHttpBinding" bindingConfiguration="ComentarioPortBinding"
                contract="SoftInvWSComentario.Comentario" name="ComentarioPort" />
			<endpoint address="http://192.168.1.37:8080/SoftInvWS/Servicio"
                binding="basicHttpBinding" bindingConfiguration="ServicioPortBinding"
                contract="SoftInvWSServicio.Servicio" name="ServicioPort" />
		</client>
	</system.serviceModel>

