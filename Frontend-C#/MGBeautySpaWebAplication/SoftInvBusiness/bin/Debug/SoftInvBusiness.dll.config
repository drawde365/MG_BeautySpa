<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ProductosPortBinding"/>
                <binding name="ProductosTipoPortBinding"/>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/SoftInvWS/Productos" binding="basicHttpBinding" bindingConfiguration="ProductosPortBinding" contract="SoftInvWSProductos.Productos" name="ProductosPort"/>
            <endpoint address="http://localhost:8080/SoftInvWS/ProductoTipo" binding="basicHttpBinding" bindingConfiguration="ProductosTipoPortBinding" contract="SoftInvWSProductoTipo.ProductosTipo" name="ProductosTipoPort"/>
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup></configuration>
